<%= form_for @kitten do |f| %>
	
	<%= f.label :name %>
	<%= f.text_field :name %>

	<%= f.label :age %>
	<%= f.select :age, (0..50).map { |i| [i, i] } %>

	<%= f.label :cuteness %>
	<%= f.select :cuteness, (0..10).map { |i| [i, i] } %>

	<%= f.label :softness %>
	<%= f.select :softness, (0..10).map { |i| [i, i] } %>

	<%= f.submit 'Submit' %>

<% end %>